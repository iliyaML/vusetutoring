<h3>Tutors ({{ totalTutors }})</h3>
<ul class="list-group" id="subjects"></ul>
{{#each tutors}}
<li class="list-group-item">
    {{#if status}}
    <span class="circle text-success" title="Available"></span>
    {{else}} 
    <span class="circle text-secondary" title="Unavailable"></span>
    {{/if}} {{ firstName }} {{ lastName }}</li>
{{else}}
<li class="list-group-item">No tutors listed.</li>
{{/each}}
</ul>